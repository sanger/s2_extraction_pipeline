{
  "workflow":{
    "role_priority":[
      "tube_to_be_extracted_nap",
       "binding_tube_to_be_extracted_nap",
       "tube_to_be_extracted_dnap",
       "binding_tube_to_be_extracted_dnap",
       "byproduct_tube_rnap",
       "binding_spin_column_dna",
       "tube_to_be_extracted_rnap",
       "binding_tube_to_be_extracted_rnap",
       "binding_spin_column_rna"
    ],

    "role_configuration":{

      "tube_to_be_extracted_nap": {
        "presenterName":  "selection_page_presenter",
        "processTitle":   "DNA and RNA Extraction",
        "input":{
          "role":"tube_to_be_extracted_nap"
        },
        "output":{
          "tube": {
            "role":         "binding_tube_to_be_extracted_nap",
            "aliquotType":  "NA+P"
          }
        }
      },

      "binding_tube_to_be_extracted_nap": {
        "presenterName":   "kit_presenter",
        "defaultPrinter":  "Tube Printer",
        "kitType":          "DNA/RNA",
        "input":{
          "role":"binding_tube_to_be_extracted_nap"
        },
        "output":{
          "tube": {
            "role":         "byproduct_tube_rnap",
            "aliquotType":  "RNA+P"
          },

          "spin_column":  {
            "role":         "binding_spin_column_dna",
            "aliquotType":  "DNA"
          }
        }
      },

      "byproduct_tube_rnap": {
        "presenterName":   "byproduct_transfer_presenter",
        "defaultPrinter":  "Tube Printer",
        "input":{
          "role":"byproduct_tube_rnap"
        },
        "output": {
          "tube": {
            "role":         "binding_tube_to_be_extracted_rnap",
            "aliquotType":  "RNA+P"
          }
        }
      },

      "binding_spin_column_dna": {
        "presenterName":   "elution_presenter",
        "defaultPrinter":  "Tube Printer",
        "input":{
          "role":"binding_spin_column_dna"
        },
        "output":  {
          "tube": {
            "role":         "extracted_tube_dna",
            "aliquotType":  "DNA"
          }
        }
      },

      "binding_tube_to_be_extracted_rnap": {
        "presenterName":   "kit_presenter",
        "defaultPrinter":  "Tube Printer",
        "kitType":         "RNA",
        "input":{
          "role":"binding_tube_to_be_extracted_rnap"
        },
        "output":  {
          "spin_column":  {
            "role":         "binding_spin_column_rna",
            "aliquotType":  "RNA"
          }
        }
      },

      "binding_spin_column_rna": {
        "presenterName":   "elution_presenter",
        "defaultPrinter":  "Tube Printer",
        "input":{
          "role":"binding_spin_column_rna"
        },
        "output":  {
          "tube": {
          "role":         "extracted_tube_rna",
          "aliquotType":  "RNA"
          }
        }
      },



      "tube_to_be_extracted_dnap": {
        "presenterName":   "selection_page_presenter",
        "processTitle":   "DNA Only Extraction",
        "input":{
          "role":"tube_to_be_extracted_dnap"
        },
        "output": {
          "tube": {
           "role":         "binding_tube_to_be_extracted_dnap",
           "aliquotType":  "DNA+P"
          }
        }
      },

      "binding_tube_to_be_extracted_dnap":{
        "presenterName":   "kit_presenter",
        "defaultPrinter":  "Tube Printer",
        "kitType":         "DNA",
        "input":{
          "role":"binding_tube_to_be_extracted_dnap"
        },
        "output": {
          "spin_column":  {
           "role":         "binding_spin_column_dna",
           "aliquotType":  "DNA"
          }
        }
      },




      "tube_to_be_extracted_rnap": {
        "presenterName":  "selection_page_presenter",
        "processTitle":   "RNA Only Extraction",
        "input":{
          "role":"tube_to_be_extracted_rnap"
        },
        "output": {
          "tube": {
           "role": "binding_tube_to_be_extracted_rnap",
           "aliquotType": "RNA+P"
          }
        }
      }

    }
  },

  "defaultPresenter": "default_presenter"
}

